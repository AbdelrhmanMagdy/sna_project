                    <div class="col-1">
                        <img class="imgBorder mb-3" height="90" width="90" src="<%= user.pic %>" style="
                                                height: 71px;
                                                width: 70px;
                                                border: 1px solid white;
                                                border-radius: 176px;
                                                "> </div>
                    <div class="col-5">
                        <a href="/visualize_users/<%= user.name %>">
